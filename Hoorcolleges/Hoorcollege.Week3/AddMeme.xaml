<Window x:Class="Hoorcollege.Week3.AddMeme"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hoorcollege.Week3"
        mc:Ignorable="d"
        Title="AddMeme" Height="118.577" Width="300"
        DataContext="{Binding Iets, Source={StaticResource Locator}}"
        >
    <StackPanel>
        <TextBlock>Naam:</TextBlock>
        <TextBox Text="{Binding Meme.Naam}"></TextBox>
        <TextBlock>Categorie:</TextBlock>
        <TextBox Text="{Binding Meme.Categorie}"></TextBox>
        <Button Command="{Binding AddCommand}">Add</Button>
        
    </StackPanel>
</Window>
